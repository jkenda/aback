include "core/stack.ab"

0 while < over 10 do
    puti dup ;
    putc ' ' ;
    + 1
end drop ;
putc '\n' ;

0 while
    peek i in < i 10 end
do
peek i in
    puti i ;
    putc ' ' ;
    + 1
end end
(10 --)

puti swap 0 ;
putc ' ' ;
(0 --)

take a in
    puti a
end

putc '\n' ;

7 13 take a b in
    puti a ; putc ' ' ;
    puti b
end

putc '\n'
